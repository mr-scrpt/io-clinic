<div class="present slider">
	<div class="present__inner swiper">
		<div class="swiper-wrapper">
			{% for item in present.present_list %}
				<div class="present__imgbox swiper-slide">
					<img src="./img/content/{{item.img}}.jpg" alt="" class="present__img img">
				</div>
			{% endfor %}
		</div>
		<div class="swiper-button-prev present__nav present__nav_prev"></div>
		<div class="swiper-button-next present__nav present__nav_next"></div>
	</div>

</div>
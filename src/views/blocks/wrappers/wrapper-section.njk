<section class="section {{inject.pageSectionClass}} page__section">
	<div class="section__inner layout__wrap {{inject.sectionInnerClass}}">
		{% if inject.title %}
			<div class="section__title title">
				<div class="title__main">{{inject.title}}</div>
			</div>
		{% endif %}

		{% block content %}{% endblock %}
	</div>
</section>